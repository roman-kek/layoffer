---
import config from "@/config/config.json";
import { Image } from "astro:assets";
const { src } = Astro.props;
const {
  logo,
  logo_width,
  logo_height,
  logo_text,
  title,
}: {
  logo: string;
  logo_width: any;
  logo_height: any;
  logo_text: string;
  title: string;
} = config.site;
---

<a href="/" class="navbar-brand block">
  {
    (
      <div class="flex justify-center items-center gap-2">
        <Image width={50} height={50} src={logo} alt={title} />
        <div>{logo_text}</div>
      </div>
    )
  }
</a>
